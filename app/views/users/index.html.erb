<div class="row">
	<div class="offset-md-3 col-md-6">
		<h1 class="title">Users</h1>
	</div>


	<div class="row index">
		<% @users.each do |user| %>
			<div class="col-md-6 index_each">
				<%= image_tag("user_images/#{user.image_name}", class: "index_image") %>
				<%= link_to user.name, user_url(user), class: "index_list" %>
			</div>
		<% end %>
	</div>
</div>